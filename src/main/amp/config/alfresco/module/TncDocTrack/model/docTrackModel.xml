<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="doctrack:model">
    <description>DocTrack Content Model</description>
    <author>TNC</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.tipinoncomuni.com/model/doctrack/1.0" prefix="doctrack"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types />
