        <aspect name="doctrack:docTrackAspect">
            <title>DocTrack Aspect</title>
            <properties>
                <property name="doctrack:QRS">
                    <title>QR identificati sul PDF</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>                    
                    <multiple>true</multiple>
                    <index enabled="true"/>
                </property>
                <property name="doctrack:zxingError">
                    <title>Errore rilevato QR zxing</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>